import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int row=-1, col=-1;
		for(int i=0; i<5; i++){
		    for(int j=0; j<5; j++){
		        int ele = sc.nextInt();
		        if(ele==1){
		            row = i;
		            col = j;
		        }
		    }
		}
		
		System.out.println(Math.abs(2-row) + Math.abs(2-col));
	}
}
